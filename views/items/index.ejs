<h2>Search Item</h2>
<form action="/items" method="GET">
    <div>
        <label>Name</label>
        <input type="'text", name="name", value="<%= searchOptions.name %> ">
    </div>
    <button type="submit">Search</button>
</form>
<br><br>
<div>
    <% items.forEach(item => { %>
        <img height="200" width = "150" src = "<%= item.itemImagePath %>">
    <% }) %> 
</div>